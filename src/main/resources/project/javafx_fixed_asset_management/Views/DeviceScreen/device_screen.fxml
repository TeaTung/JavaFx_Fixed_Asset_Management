<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: white;"
            xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="project.javafx_fixed_asset_management.Controllers.DeviceScreenController">
    <children>
        <TableView fx:id="deviceTableView" layoutX="31.0" layoutY="97.0" prefHeight="542.0" prefWidth="1219.0">
            <columns>
                <TableColumn fx:id="idColumn" minWidth="150" prefWidth="0" text="ID"/>
                <TableColumn fx:id="deviceNameColumn" maxWidth="150.0" prefWidth="141.0" text="Device Name"/>
                <TableColumn fx:id="quantityColumn" minWidth="0.0" prefWidth="116.60000610351562" text="Quantity"/>
                <TableColumn fx:id="unitColumn" minWidth="0.0" text="Unit"/>
                <TableColumn fx:id="typeOfDeviceColumn" minWidth="0.0" prefWidth="116.60000610351562" text="Model"/>
                <TableColumn fx:id="priceColumn" minWidth="0.0" prefWidth="92.0" text="Price"/>
                <TableColumn fx:id="specificationColumn" minWidth="0.0" prefWidth="148.0" text="Specification"/>
                <TableColumn fx:id="statusColumn" minWidth="0.0" prefWidth="105.0" text="Status"/>
                <TableColumn fx:id="percentDamage" minWidth="0.0" prefWidth="82.0" text="Percent Damage"/>
                <TableColumn fx:id="dateOfUsingColumn" minWidth="0.0" prefWidth="147.199951171875"
                             text="Date of using"/>
                <TableColumn fx:id="dateOfManufacturingColumn" minWidth="0.0" prefWidth="156.0"
                             text="Date of Manufacturing"/>
            </columns>
        </TableView>
        <Button fx:id="addBtn" layoutX="950.0" layoutY="657.0" mnemonicParsing="false" onAction="#addDeviceButtonAction"
                prefHeight="33.0" prefWidth="89.0" text="Add" AnchorPane.bottomAnchor="30.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Button fx:id="updateBtn" layoutX="1160.0" layoutY="657.0" mnemonicParsing="false"
                onAction="#updateDeviceButtonAction" prefHeight="33.0" prefWidth="89.0" text="Update"
                AnchorPane.bottomAnchor="30.0" AnchorPane.rightAnchor="30.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Button fx:id="backBtn" layoutX="31.0" layoutY="657.0" mnemonicParsing="false"
                onAction="#backDeviceButtonAction" prefHeight="33.0" prefWidth="89.0" text="Back"
                AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="30.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Button fx:id="deleteBtn" layoutX="1055.0" layoutY="657.0" mnemonicParsing="false"
                onAction="#deleteDeviceButtonAction" prefHeight="33.0" prefWidth="89.0" text="Delete"
                AnchorPane.bottomAnchor="30.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <TextField fx:id="searchTF" layoutX="567.0" layoutY="657.0" onKeyPressed="#onKeyTypeSearchTF" prefHeight="33.0"
                   prefWidth="260.0" promptText="Device name" AnchorPane.bottomAnchor="30.0"/>
        <Button fx:id="searchBtn" layoutX="844.0" layoutY="657.0" mnemonicParsing="false"
                onAction="#searchDeviceButtonAction" prefHeight="33.0" prefWidth="89.0" text="Search"
                AnchorPane.bottomAnchor="30.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Text layoutX="43.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="lbl-primary"
              text="SELECT DEVICE TO BE INVENTORY" wrappingWidth="507.0" AnchorPane.topAnchor="37.0224609375">
            <font>
                <Font name="System Light" size="25.0"/>
            </font>
        </Text>
    </children>
</AnchorPane>
